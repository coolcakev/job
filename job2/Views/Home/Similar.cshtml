<link rel="stylesheet" href="~/css/site.css" />

@model List<List<announcement>>
@{
    ViewBag.Title = "Все annoncement";
}

@{
    for (int i = 0, cou = 0; i < Model.Count; i++)
    {

        if (Model[i].Count > 1)
        {

            <p> @(cou++) спроба </p>

            for (int j = 0; j < Model[i].Count; j++)
            {
                <p> @Model[i][j].Title</p>
                <p> @Model[i][j].Description</p>
                <p> @Model[i][j].Date_Added</p>
               
            }





        }



    }

}
